const cotizacion=(costo,utilidad)=>{
    return costo/(1-(utilidad*.01))
}

const calcutilidad=(costo,venta)=>{
    let utilidad=((venta-costo)/venta)
    return utilidad
}

console.log(cotizacion(alcohol,22))
console.log(cotizacion(acetona,15))
